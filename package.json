{
  "name": "wwarby-com",
  "version": "1.0.0",
  "description": "wwarby.com website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwarby/wwarby.com.git"
  },
  "keywords": [
    "wwarby",
    "wwarby.com",
    "website",
    "angular"
  ],
  "author": "William Warby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wwarby/wwarby.com"
  },
  "homepage": "https://github.com/wwarby/wwarby.com#readme",
  "engines": {
    "node": ">= 10.6.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --extract-css --base-href /",
    "build-prod": "ng build --prod --base-href /",
    "deploy": "npm run qc && npm run build-prod && ts-node scripts/deploy prod && rimraf dist",
    "docker": "docker build -t wwarby-com . && docker run -d -p 4200:80 wwarby-com",
    "sass-lint": "node ./node_modules/sass-lint/bin/sass-lint.js -c .sass-lint.yml -v -q",
    "tslint": "ng lint wwarby-com --fix && ng lint wwarby-com-e2e --fix",
    "lint": "npm run tslint && npm run sass-lint",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e",
    "qc": "npm run lint && ng test --browsers ChromeHeadless --watch false && ng e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "ncu": "ncu -u -x typescript && npm update && npm i",
    "cc": "open ./coverage/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/material": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "@fortawesome/fontawesome-free": "5.15.4",
    "angular-in-memory-web-api": "0.11.0",
    "angulartics2": "11.0.0",
    "core-js": "3.1.3",
    "hammerjs": "2.0.8",
    "ngx-image-gallery": "2.0.5",
    "normalize.css": "8.0.1",
    "rxjs": "6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.2",
    "@angular/cdk": "8.0.0",
    "@angular/cli": "8.0.2",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/hammerjs": "2.0.40",
    "@types/jasmine": "3.10.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.9",
    "codelyzer": "6.0.2",
    "coveralls": "3.1.1",
    "fs": "0.0.2",
    "ftp-deploy": "2.4.1",
    "jasmine-core": "3.10.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lodash": "4.17.21",
    "path": "0.12.7",
    "protractor": "7.0.0",
    "sass-lint": "1.13.1",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "typescript": "4.4.4"
  }
}
